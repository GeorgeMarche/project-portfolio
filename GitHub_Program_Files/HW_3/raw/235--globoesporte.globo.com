function getApiAttribute(cadunUserMe) {
  let urlApiAttribute =
    endpointAttributeApi + "/v1/attribute/glbid/" + cadunUserMe.token;

  axios
    .get(urlApiAttribute)
    .then(function(response) {
      if ( response.data.time_futebol_coracao === undefined ) {
        setApiAttribute(cadunUserMe);
      }
    }).catch(function(error) {
      console.error("Erro ao recuperar time_futebol_coracao", error);
    });
}


function setApiAttribute(cadunUserMe) {
  let userTeam = JSON.parse(localStorage.getItem("userTeam"));
  let urlApiAttribute = endpointAttributeApi + "/v1/attribute/glbid";

  if (userTeam !== null) {
    let userNameTeam = userTeam.userTeam.name || "";
    let requestData = {
      "glbid": cadunUserMe.token,
      "key": "time_futebol_coracao",
      "value": userNameTeam
    };
    let header = {
      "Client-Id": "ge",
      "Content-Type": "application/json"
    };

    axios({
      method: "put",
      url: urlApiAttribute,
      data: requestData,
      headers: header
    }).then(response => {
      localStorage.setItem("userTeamAttributes", true);
      sendToGA("time-futebol-coracao",'put',userNameTeam);
      console.info("time_futebol_coracao", response);
    }).catch(error => {
      sendToGA("time-futebol-coracao", "put error", userNameTeam);
      console.error("Erro ao definir time_futebol_coracao", error);
    });
  }
}


