				var NLAsaja = {
					error : $asaja(".asaja_newsletter .error"),
					mostrarError : function(mensaje) {
						var _this = this;
						_this.error.html(mensaje);
						_this.error.fadeIn(function() {
							window.setTimeout(function() {
								_this.error.fadeOut();
							}, 5000);
						});
					},
					
					opaca : function() {
						var _this = this;
						
						var opaca = $asaja("<div>").attr("id", "nl_opaca");
						opaca.css({
							background : '#fff',
							height : $asaja(".asaja_newsletter").height() + 'px',
							left : 0,
							position : 'absolute',
							opacity : '.8',
							top : 0,
							width : $asaja(".asaja_newsletter").width() + 'px',
							zIndex : 2
						});
						
						$asaja(".asaja_newsletter").append(opaca);
					},
					
					submit : function() {
						var _this = this;
						
						_this.error.html("");
						var nl_email = $asaja.trim($asaja("#nl_email").val());
						var socio = $asaja("#nl_socio").get(0).checked;
						var nl_logged_in = ($asaja("#nl_logged_in").val() == 1);
						
						if (nl_email == "") {
							_this.mostrarError("El correo electrónico no puede estar vacio");						
							return false;
						}
						
						if (/^[a-z0-9!#$%&'*+/=?^_`{|}~-]+(?:\.[a-z0-9!#$%&'*+/=?^_`{|}~-]+)*@(?:[a-z0-9](?:[a-z0-9-]*[a-z0-9])?\.)+[a-z0-9](?:[a-z0-9-]*[a-z0-9])?$/.test(nl_email) == false) {
							_this.mostrarError("El correo electrónico es incorrecto");
							return false;
							
						}
						
						if (socio == true && !nl_logged_in) {
							_this.mostrarError("Inicie sesión para marcar la casilla de socio");
							return false;
						}
						
						_this.opaca();
						
						$asaja.post('/wp-content/plugins/asajajaen-plugin/asaja-newsletter-guardar.php', {
							nl_accion : 'guardar',
							nl_email : nl_email,
							nl_socio : socio
						}, function(data) {
							$asaja("#nl_opaca").remove();
							_this.mostrarError(data);
						});
					}
				};
				
				$asaja("#nl_asaja").on("submit", function() {
					NLAsaja.submit();
					return false;
				});
			</script></div></div>
