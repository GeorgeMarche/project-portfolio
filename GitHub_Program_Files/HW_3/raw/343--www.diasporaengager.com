				} else {
					//alert($("#register-form").serialize());
					$("#register-info1").hide();
					$("#register-error1").hide();
					$("#register-success1").hide();
					$("#register-info1").html("Processing...");
					$("#register-info1").fadeIn();
					$("#btn-register1").attr("disabled", "disabled");					
					$.post("/register/register_process", $("#register-form1").serialize(), function(data) {
						setTimeout(function() {
							$("#register-info1").fadeOut(1000, function() {
								$("#register-success1").html("Please check email to continue.");							
								$("#register-success1").fadeIn(500);
							});
						}, 1500);
					});		
			        return false;
				}
				
			});
			return false;
				
	    });	

	
});
</script>
<script>
		
		$("#register-type-org").click(function() {
			$("#register-user").hide();
			$("#register-org").show();
			$("#registerType").val("2");
		});

		$("#register-type-user").click(function() {
			$("#register-org").hide();
			$("#register-user").show();
			$("#registerType").val("1");
		});
		
		$("#login-form").submit(function() {

			$("#login-info").hide();
			$("#login-error").hide();
			$("#login-success").hide();
			$("#login-info").html("Authenticating...");
			$("#login-info").fadeIn();
			//$("#btn-login").attr("disabled", "disabled");
			$.post("/register/login_process",{ username:$('#username').val(),password:$('#password').val(),rand:Math.random() } ,function(data) {
				if(data=='yes') {
					setTimeout(function() {
						$("#login-info").fadeOut(500, function() {
							$("#login-success").html("Success, logging in...");
							$("#login-success").fadeIn(500);
							setTimeout(function() {
								 document.location='/register/welcome';
							}, 2500);
						});
					}, 1500);
				} else if (data=='dash') {	
					setTimeout(function() {
						$("#login-info").fadeOut(500, function() {
							$("#login-success").html("Success, logging in...");
							$("#login-success").fadeIn(500);
							setTimeout(function() {
								 document.location='/dashboard/';
							}, 2500);
						});
					}, 1500);	
				} else if (data=='blocked') {	
					setTimeout(function() {
						$("#login-info").fadeOut(500, function() {
							$("#login-error").html("Something may be wrong, please contact us.");
							$("#login-error").fadeIn(500);
							$("#btn-login").removeAttr("disabled", "");
						});
					}, 1500);				
				} else {
					setTimeout(function() {
						$("#login-error").html("Login failed. Please check Email Address and/or Password and try again.");
						$("#login-info").fadeOut(500, function() {
							$("#login-error").fadeIn(500);
							$("#btn-login").removeAttr("disabled", "");
						});						
					}, 2500);
					setTimeout(function() {
						$("#login-error").fadeOut(500);
					}, 20000);
				}
			});		
	        return false;

	    });
	
		$("#register-form").submit(function() {
			var regType  = $("#registerType").val();
			var regComp  = $("#registerCompany").val();
			var fname 	 = $("#registerFname").val();
			var lname 	 = $("#registerLname").val();
			var email 	 = $("#registerEmail").val();
			var password = $("#registerPassword").val();
			$.post("/register/check_username?email="+email, function(data) {
				if (data=="no") {
					$("#register-error").fadeIn();
					$("#register-error").html("Email address is already being used!");	
				} else if (regType==1 && (fname=="" || lname=="" || email=="" || password=="")) {
					$("#register-error").fadeIn();
					$("#register-error").html("Please fill in all required fields");
				} else if (regType==2 && (regComp=="" || email=="" || password=="")) {
					$("#register-error").fadeIn();
					$("#register-error").html("Please fill in all required fields");
				} else {
					//alert($("#register-form").serialize());
					$("#register-info").hide();
					$("#register-error").hide();
					$("#register-success").hide();
					$("#register-info").html("Processing...");
					$("#register-info").fadeIn();
					$("#btn-register").attr("disabled", "disabled");					
					$.post("/register/register_process", $("#register-form").serialize(), function(data) {
						setTimeout(function() {
							$("#register-info").fadeOut(1000, function() {
								$("#register-success").html("Please check your email inbox to continue. ");							
								$("#register-success").fadeIn(500);
							});
						}, 1500);
					});		
			        return false;
				}
				
			});
			return false;
				
	    });	
		</script>
</body>
</html>
