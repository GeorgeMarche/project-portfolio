            /*custom css */
            /* ################################################### */
/* MAIN */
/* ################################################### */
 .tn-main-container, .tn-container {
     max-width : 1200px;
}
img{
    border-radius: 2px;
}
 header {
     margin-bottom : 30px;
}
 .site-logo {
     width : 310px;
}
 .header-ads-wrap img {
     width : 700px;
     opacity : 0.6;
     filter: alpha(opacity=60);
    /* For IE8 and earlier */
     border-radius : 5px;
     box-shadow : 0px 0px 10px 5px rgb(106, 146, 160);
     margin-right: 10px;
}
 #tn-menu-top-inner {
     font-size: 12px;
}
 #signup-content{
 margin-top: 60px;   
}
 .main-nav-wrap {
    z-index: 888;
}
 .menu-nav-top li a {
     padding: 0 5px;
}
.menu-top-right{
    margin: auto;
    padding: 0;
}
 #main-header {
     padding : 5px 0;
}
 #main-content {
     padding : 0px;
}
 #sidebar {
     padding : 0px 0px 15px 20px;
}
 #sidebar .widget-title {
     margin-bottom : 10px;
}
aside#recent-posts-2 > ul > li > span.post-date {
     color: #1174d0; /* bleu clair */
}
 #main-content .widget-title h3, #sidebar .widget h3 {
     font-weight : 700;
     color : #FFFFFF;
}
 .next-prev-wrap {
     padding-right : 10px;
}
 .next-prev-wrap a {
     color : #FFFFFF;
}
 #footer {
     background : #050c50;
}
 .login-widget-wrap .author-title {
     color: #FFFFFF!important;
}
 #footer input[type="text"], #footer input[type="email"], #footer input[type="password"] {
     background : #444;
     border-color : #444;
}
#footer a {
    /* espacement dans le bloc "Auteurs" */
    padding-left: 5px;
}

 #menu-main > ul > li > a {
     padding : 0 17px;
     font-size: 13px;
}

/* icone pour remonter en haut du site*/
#toTop i {
    height: 65px;
}

/* ################################################### */
/* SPECIFIC EFFECTS */
/* ################################################### */
/* Barre de menu/rubriques fixe lors du scroll */
 #main-nav.fixed-nav {
     position:fixed;
     top:-5px;
     z-index:999;
     opacity:0.9;
     box-shadow:0 5px 3px rgba(0, 0, 0, .1);
}
/* Alignement du wrapper*/
 #main-wrapper {
     padding-top: 20px;
}
/* ----------------------------------*/
/* Ajustement de la taille des images du carroussel */
 div .big-carousel-content img {
     height : 100px;
     width : 212px;
}
 #main-content .block-big-slider-content-wrap {
     padding : 15px 10px 0px 10px;
}
/* Opacité des flèches du slider */
 .tn-direction-nav li a {
     opacity : 0.9;
}
/* Cachage des chiffres (pbm d'affichage des sous-images du slider sur mobile) */
