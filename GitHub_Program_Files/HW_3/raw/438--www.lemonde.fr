            /* on boucle sur les données pour les formatter */
        radprec = 0
        dcprec = 0
        dataTemp.forEach(function(d, i) {
            if (d[2]) {
                cumul = +(d[5] - dcprec)
                parjour = +d[2] /* + d[3]*/
                if (cumul > maxParJour || parjour > maxParJour) {
                    maxParJour = Math.max(cumul, parjour)
                }

