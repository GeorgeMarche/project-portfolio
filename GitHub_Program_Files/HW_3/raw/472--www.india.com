    <script>
        jQuery(document).ready(function() {
            jQuery("input[name='return_date']").click(function() {
                jQuery("#fv-booking").hide();
            });;
            jQuery("input[name='depart_date']").click(function() {
                jQuery("#fv-booking").hide();
            });

            jQuery("#passangers").click(function() {
                jQuery("#fv-booking").toggle();
            });
            jQuery("#btnPaxSubmit").click(function() {
                jQuery("#fv-booking").hide();
            });

            jQuery('input[name="class"]').change(function() {
                var id = jQuery(this).attr('id');
                var label = jQuery("label[for='" + id + "']").text();
                var passangers = getTotalPax();
                jQuery("#passangers").val(passangers + ' Travelers , ' + label);
            });

        });

        function SetAddPax(objElement, prefix) {
            var value = jQuery('input[name="' + objElement + '"]').val();

            value = (value == "" ? 0 : parseInt(value));

            if (prefix == '+') {
                value = value + 1;
            } else if (prefix == '-') {
                value = value - 1;
            }

            jQuery('input[name="' + objElement + '"]').val(value);
            jQuery('#' + objElement).text(value);
            var selectedClass = jQuery("input[name='class']:checked").attr('id');
            jQuery("#passangers").val(getTotalPax() + " Travelers, " + jQuery("label[for='" + selectedClass + "']").text());

        }

        function getTotalPax() {
            var adults = jQuery('input[name="adult"]').val();
            adults = (adults == "" ? 0 : parseInt(adults));
            var childs = jQuery('input[name="child"]').val();
            childs = (childs == "" ? 0 : parseInt(childs));
            var infants = jQuery('input[name="infant"]').val();
            infants = (infants == "" ? 0 : parseInt(infants));

            var total_pax = adults + childs + infants;
            return total_pax;
        }
        jQuery("#search").click(function(event) {

            jQuery("#frmDetails").validate();
            var is_valid = jQuery("#frmDetails").valid();
            if (is_valid) {

                var adults = jQuery('input[name="adult"]').val();
                var childs = jQuery('input[name="child"]').val();
                var infants = jQuery('input[name="infant"]').val();
                var origin = jQuery('input[name="origin"]').val();
                var destination = jQuery('input[name="destination"]').val();
                var flight_class = jQuery('input[name="class"]').val();
                var depart_date = jQuery('input[name="depart_date"]').val();
                var return_date = jQuery('input[name="return_date"]').val();
                var JourneyType = jQuery('input[name="JourneyType"]').val();
                var location_url = 'https://tripexplorer.tripprosites.com/sites/flights/search/home';
                var from = origin.split(',');
                location_url += "/" + ((from[0] != '' && from[0] != 'undefind') ? from[0] : '');
                var to = destination.split(',');
                location_url += "-" + ((to[0] != '' && to[0] != 'undefind') ? to[0] : '');
                depart_date = jQuery.datepicker.formatDate('mmddyy', new Date(depart_date));
                location_url += "/" + depart_date + "-";
                return_date = return_date != '' ? jQuery.datepicker.formatDate('mmddyy', new Date(return_date)) : 'undefined';
                location_url += return_date;
                location_url += "/" + ((adults != '' && adults != 'undefind') ? adults : 0);
                location_url += '-' + ((childs != '' && childs != 'undefind') ? childs : 0);
                location_url += '-' + ((infants != '' && infants != 'undefind') ? infants : 0);
                location_url += '/' + ((flight_class != '' && flight_class != 'undefind') ? flight_class : 0);
                location_url += '-' + ((JourneyType != '' && JourneyType != 'undefind') ? JourneyType : 0);
                var jcType = 'one-way';
                if(JourneyType==1){
                	jcType = 'round-trip';
                }
                if(JourneyType==2){
                	jcType = 'multi-city';
                }
                if(JourneyType==0){
                	jcType = 'one-way';
                }
                gtag('event', 'click-search-flights', {'event_category' : 'flight-widget', 'event_label' : jcType});
                //window.location = location_url;
                window.open(location_url,'_blank');

            } else {
                return false;
            }
        });

        jQuery(document).ready(function() {
                jQuery.getJSON("https://zeemundo.com/airport_code.php", function(data) {
    
                autoComplete = [];
                for (var i = 0, len = data.length; i < len; i++) {
                    autoComplete.push(data[i].Airport_Code + ", " + data[i].City_Name + ", " + data[i].Country_Code);
                }
                jQuery("#origin").autocomplete({
                    source: function(request, response) {
                        
                        var results = jQuery.ui.autocomplete.filter(autoComplete, request.term);
                        response(results.slice(0, 10));
                    },

                });
                jQuery("#destination").autocomplete({
                    source: function(request, response) {
            
                        var results = jQuery.ui.autocomplete.filter(autoComplete, request.term);
                        response(results.slice(0, 10));
                    },

                });
            });
        });
        jQuery(function() {
            jQuery("input[name='return_date']").datepicker({
                minDate: 0,
                startDate: '-1d',
            });
            jQuery("input[name='depart_date']").datepicker({
                autoclose: true,
                format: 'dd/mm/yyyy',
                todayHighlight: true,
                minDate: 0,
                onSelect: function(selected) {
                    jQuery("input[name='return_date']").datepicker("option", "minDate", new Date(selected));
                }
            });
        });
        jQuery('#JourneyTypeUL li a').click(function() {
            jQuery('li a').removeClass();
            jQuery(this).addClass('active');
            var JourneyType = jQuery(this).attr("data-id");
            jQuery("#JourneyType").val(JourneyType);
            if (JourneyType == 0) {
                jQuery("#return_date").attr('disabled', 'disabled');
                jQuery("#return_date").attr('required', 'required');
            } else {
                jQuery("#return_date").removeAttr('disabled', 'disabled');
                jQuery("#return_date").removeAttr('required', 'required');
            }
        });
    </script>
