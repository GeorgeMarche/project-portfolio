            function cambiarIcono(id){
                switch(id){
                    case "icono_menu":
                        if($("#"+id).hasClass("fa-bars")) {
                            $("#"+id).removeClass("fa-bars");
                            $("#"+id).addClass("fa-times");
                        }else{
                            $("#"+id).removeClass("fa-times");
                            $("#"+id).addClass("fa-bars");
                        }
                        break;
                    default:
                        if($("#"+id).hasClass("fa-angle-down")){
                            $("#"+id).removeClass("fa-angle-down");
                            $("#"+id).addClass("fa-angle-up");
                        }else{
                            $("#"+id).removeClass("fa-angle-up");
                            $("#"+id).addClass("fa-angle-down");
                        }
                        break;
                }
            }

            function abrir_cerrar_buscador(id) {
                var elemento = document.getElementById(id);

                if(elemento.style.display=="none") {
