function contact_table() {
  $('table caption:contains(CON_TAB), table caption:contains(PAY_TAB)').each(function(){
    if ($(this).parent().hasClass('er-tbl-acc') == false) {
      $(this).parent().addClass('er-tbl-acc');
    }
    if ($(this).parent().find('tbody tr:nth-child(odd)').hasClass('er-tbl-acc-tr-type') == false) {
      $(this).parent().find('tbody tr:nth-child(odd)').addClass('er-tbl-acc-tr-type').find('td').append($('<i class="fa icon-right fa-chevron-down" aria-hidden="true"></i>'));
    }
    if ($(this).parent().find('tbody tr:nth-child(even)').hasClass('er-tbl-acc-tr-content') == false) {
      $(this).parent().find('tbody tr:nth-child(even)').addClass('er-tbl-acc-tr-content').find('td').wrapInner('<div/>');;
    }
    if ($(this).parent().find('tbody tr:nth-child(4n), tbody tr:nth-child(4n-1)').hasClass('even') == false) {
      $(this).parent().find('tbody tr:nth-child(4n), tbody tr:nth-child(4n-1)').addClass('even');
    }
    if ($(this).parent().find('tbody tr:nth-child(4n-3), tbody tr:nth-child(4n-2)').hasClass('odd') == false) {
      $(this).parent().find('tbody tr:nth-child(4n-3), tbody tr:nth-child(4n-2)').addClass('odd');
    }
    $(this).parent().find('tbody tr:nth-child(odd)').each(function(){
      var attr = '';
      if ($(this).parents('table').find('caption:contains(CON_TAB)').length > 0) {
        attr = $(this).next().text().trim().replace(/\s\s+/g, " ").replace(':', '').replace('Telephone', 'Phone');
        if ($(this).find('> td:contains(Text message)').length > 0 || $(this).find('> td:contains(Phone)').length > 0 || $(this).find('> td:contains(Fax)').length > 0) {
          attr = $(this).next().text().trim().replace(/[\s()]/g, "").replace(':', '').replace('Telephone', 'Phone');
        }
      }
      else if ($(this).parents('table').find('caption:contains(PAY_TAB)').length > 0) {
        attr = $(this).text().trim().replace(':', '').replace('Telephone', 'Phone');
      }
      var action = $(this).text().trim().replace(':', '').replace('Telephone', 'Phone');
      if ($(this).find('div').length == 0) {
        $(this).find('td').wrapInner('<div><span aria-label="Show or Hide the details for ' + $(this).text().trim() + '" role="link"></span></div>');
      }
      if ($(this).find('.er-tbl-overlay').length == 0) {
        $(this).find('td').prepend('<div class="er-tbl-overlay" data-action="' + action + '" data-content="' + attr + '"></div>');
      }
    })
    var tbl_icons = {
      "Text message":                         ["Text message", "fa-mobile-alt fas", ""],
      "Facebook":                             ["Facebook", "fa-facebook-square fab"],
      "Twitter":                              ["Twitter", "fa-twitter fab"],
      "Fax":                                  ["Fax", "fa-fax far", ""],
      "Phone":                                ["Phone", "fa-phone fas", ""],
      "Email":                                ["Email", "fa-envelope far", ""],
      "Address":                              ["Address", "fa-map-marker-alt far", ""],
      "Online":                               ["Online", "fa-globe-americas far", ""],
      "Direct Debit":                         ["Direct Debit", "fa fa-blank.direct-debit", ""],
      "Internet banking and standing order":  ["Internet banking and standing order", "fa-university fas", ""],
      "Customer service centre":              ["Customer service centre", "fa-users fas", ""],
      "PayPoint":                             ["PayPoint", "fa fa-blank.paypoint", ""],
      "Post":                                 ["Post", "fa-paper-plane far", "Office"],
      "Post Office":                          ["Post Office", "fa-mail-bulk far", ""],
    }
    var $this = $(this);
