$(document).ready(function() {
  
  if ($('.er-accordion-disabled').length == 0) {
    
    $('.er-accordion-block').each(function(index) {
      
      if ($(this).find('.er-accordion-title').text().length == 0) {
        $(this).remove();
      }
    });
    $('.er-accordion-block').each(function(index) {
      
      if ($(this).find('.er-accordion-title').text().length > 0) {
        
        var er_accordion_readon_wrapper = '<div class="er-accordion-readon"></div>';
        var er_accordion_totop = '<a href="#top-of-page" class="er-accordion-totop"><span>Top of page</span><i aria-hidden="true" class="fa fa-fw fa-angle-double-up"></i></a>';
        if ($(this).find('.er-readmore-replace').length > 0) {
          $(this).find('.er-accordion-readon').addClass('hidden');
        }

        $(this).find('.ContentEditor').find('span, a, p, li, ul').removeAttr('style');

        
        $(this).find('.er-container > .er-padding').prepend(er_accordion_readon);
        $(this).find('.er-accordion-readon').append(er_accordion_totop);

        
        var index_adj = index + 1;
        var er_index = $('.er-accordion-block:nth-child(' + index_adj + ')');

        
        if ($(this).find('.er-accordion-content').text().length > 0) {

          
          var er_accordion_readon = '<a href="" class="er-accordion-readmore"><span>Read more</span><i aria-hidden="true" class="fa fa-fw fa-chevron-down"></i></a><a href="" class="er-accordion-readless hidden"><span>Read less</span><i aria-hidden="true" class="fa fa-fw fa-chevron-up"></i></a>';
          $(this).find('.er-accordion-readon').prepend(er_accordion_readon);

          
          function er_accordion_controls(selector) {

            
            var max_height_clone = selector.find('.er-accordion-content').clone().attr('id', 'er-accordion-content-' + index).css({"max-height":"100%","width":"auto","position":"absolute","visibility":"hidden"}).insertAfter(selector.find('.er-accordion-content'));
            var max_height = 0;
            $('#er-accordion-content-' + index).children().each(function(){
              max_height += $(this).outerHeight(true);
            })
            $('#er-accordion-content-' + index).remove();

            var tbl_con_height_adj = 0;

            if (selector.find('.er-accordion-content .er-tbl-acc').length > 0) {
              var tbl_con_height_init = selector.find('.er-accordion-content .er-tbl-acc').height();
              var tbl_con_clone = selector.find('.er-accordion-content .er-tbl-acc').clone().addClass('clone').attr('id', 'er-accordion-content-' + index + '-tbl-con').css({"max-height":"100%","width":"100%","position":"absolute","visibility":"hidden"}).insertAfter(selector.find('.er-accordion-content .er-tbl-acc'));
              var tbl_con_height_max = $('#er-accordion-content-' + index + '-tbl-con').height();
              tbl_con_height_adj = tbl_con_height_max - tbl_con_height_init;
              $('#er-accordion-content-' + index + '-tbl-con').remove();
            }

            max_height = max_height + tbl_con_height_adj;

            if (selector.find('.er-accordion-readmore').hasClass('hidden')) {
              selector.find('.er-accordion-readmore').removeClass('hidden');
              selector.find('.er-accordion-readless').addClass('hidden');
              selector.find('.er-accordion-content').css("max-height", 0).removeClass('open');
              selector.find('.er-accordion-content a').attr('tabindex', '-1');
              var target_scroll = selector.find('.er-accordion-title');
              $.smoothScroll({
                scrollTarget: target_scroll[0],
                offset: -50
              });
            }
            else if (selector.find('.er-accordion-readless').hasClass('hidden')) {
              selector.find('.er-accordion-readless').removeClass('hidden');
              selector.find('.er-accordion-readmore').addClass('hidden');
              selector.find('.er-accordion-content').css("max-height", max_height * 1.09).addClass('open');
              selector.find('.er-accordion-content a').removeAttr('tabindex');
              var target_scroll = selector.find('.er-accordion-content');
              $.smoothScroll({
                scrollTarget: target_scroll[0],
                offset: -50
              });
            }
          }

          
          $(this).find('.er-accordion-readon a.er-accordion-readmore, .er-accordion-readon a.er-accordion-readless').click(function(event){
            event.preventDefault();
            er_accordion_controls(er_index);
          });

          
          var block = $(this);
          $('.er-content-area a[href*="#"]:not(.er-readmore-replace)').click(function(){
            var anchor = $(this).attr('href');
            if (block.find(anchor).parents('.er-accordion-content').length > 0 && block.find(anchor).parents('.er-accordion-content').siblings('.er-accordion-readon').find('> .er-accordion-readless').hasClass('hidden') == true) {
              er_accordion_controls(block.find(anchor).parentsUntil('.er-accordion-block'));
            }
          })
        }

        
        else {
          $(this).addClass('er-accordion-block-disabled');
        }
      }
    });
    
    
    $('.er-accordion-totop').smoothScroll();

    
    $('.er-accordion-title h2:not(.title) a').click(function(event){
      event.preventDefault();
      $('html, body').animate({scrollTop: $(this).offset().top - 30}, 500);
      var tgt_bg = $(this).closest('.er-accordion-block').css('background-color');
      $(this).closest('.er-accordion-block').animate({ backgroundColor: "#CCCCCC"}, 500, 
        function () {
          $(this).closest('.er-accordion-block').animate({backgroundColor: tgt_bg});
        });
    })
    $('.er-accordion-title h2:not(.title)').each(function() {
      $(this).find('br').remove();
    })
    $('.er-accordion-title h2:not(.title) a').each(function() {
      // Automatically add link if missing
      if (($(this).attr('id')==null || $(this).attr('id').length < 1) && $(this).attr('href')!=null) {
        var href = $(this).attr('href').slice(1);
        $(this).attr('id', href);
      }
      $(this).removeAttr('style');
      // Remove EM
      if ($(this).find('em').length > 0) {
        var html = $(this).find('em').html();
        $(this).html(html);
      }
      if ($(this).parent().is('em') == true) {
        var html = $(this).parent().html();
        $(this).parents().eq(1).html(html);
      }
    })
    
    $('.er-accordion-title h2:not(.title) a:not(.panel-event)').each(function() {
      if ($(this).find('em').length == 0) {
        $(this).wrapInner('<em>');
      }
    })
    $('.er-accordion-content a').each(function(){
      $(this).attr('tabindex', '-1');
    })
    
  }
  $('.er-readmore-replace').click(function(e){
    e.preventDefault();
    $(this).toggleClass('open');
    $(this).parents('.er-accordion-title').siblings('.er-accordion-readon').find('a[class^="er-accordion-read"]:not(.hidden)').click();
  })
});


function zoom() {
  if (screen.width > window.innerWidth || window.devicePixelRatio > 1){
    $("body").addClass("er-zoom-in").removeClass("er-zoom-out");
  }
  else if (screen.width < window.innerWidth || window.devicePixelRatio < 1){
    $("body").addClass("er-zoom-out").removeClass("er-zoom-in");
  }
  else {
    $("body").removeClass("er-zoom-out").removeClass("er-zoom-in");
  }
}
