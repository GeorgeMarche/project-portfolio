<script type="text/javascript">
function er_dt_parseResponses(responses) { // takes a responses array and turns it into a <ol>
  var ol = document.createElement('ol');
  ol.setAttribute('data-count', responses.length);
  for (var i=0; i < responses.length; i++) {
    ol.appendChild(er_dt_parseResponse(responses[i]));
  }
  return ol;
}

function er_dt_parseResponse(response) { // takes a response object and turns it into a <li>
  var li = document.createElement('li');
  li.className = response.category;
  if (typeof response.id != 'undefined') {
    li.className = response.category + ' ' + response.id;
  }
  if (response.category == 'output') {
    li.innerHTML = '<div class="er-dt-result">' + response.responses+ '</div>';
  }
  else if (response.category == 'function') {
    li.innerHTML = '<div class="er-dt-result"><div class="hidden">' + response.responses+ '</div><div class="replace"></div></div>';
  }
  else if (response.category == 'question') {
    li.innerHTML = '<h3 class="er-nomargintop">' + response.name + '</h3>';
  }
  else if (response.category == 'answer') {
    if (typeof response.click != 'undefined') {
      li.innerHTML = '<a href="" class="er-link-styled er-dt-answer" data-click="' + response.click +'">' + response.name + '</a>';
    }
    else {
      li.innerHTML = '<a href="" class="er-link-styled er-dt-answer">' + response.name + '</a>';
    }
  }

  if (response.responses && response.category != 'output' && response.category != 'function') {
    li.appendChild(er_dt_parseResponses(response.responses))
  }

  if (response.category != 'answer') {
    li.insertAdjacentHTML('beforeend', '<a href="" class="er-dt-back er-button-5 er-button-small er-nomarginbottom">Back</a>');
  }
  
  return li;
}

function er_dt_loader() {
  if ($('.er-dt-placeholder').length > 0) {
    $('.er-dt-placeholder:not(.loaded)').each(function(idx, obj){
      var file = $(this).attr('data-map');
