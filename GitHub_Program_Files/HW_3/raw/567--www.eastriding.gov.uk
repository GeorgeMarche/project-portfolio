      $(this).attr('data-idx', idx);

      $.getScript('/EasySiteWeb/EasySite/StyleData/base-extensions/decision-tree-content/' + file + '/structure.js?v=1.6.0.6', function(data){
        var structure_name = 'structure_' + file;
        var dt_output = er_dt_parseResponses(eval(structure_name));
        $this.append(dt_output);
        $this.addClass('loaded');
        $this.trigger('er_dt_loaded');
      })

    })
  }
}
