<script type="text/javascript">
// Functions
var er_menu_dur         = 300; // Mobile pullout duration
var er_menu_top_dur     = 100; // Mobile menu to top button fade duration
var sidepanel_width     = '17rem'; // Width of sidepanel
var sidepanel_dur       = 175; // Sidepanel animation duration

$(document).ready(function(){

  
  $('#er-sidepanel .er-menu-drilldown-1 ul a').attr('tabindex', '-1');
  

  // Tablet/mobile menu slide out
  // Menu button
  $("#er-menu-closed").click(function(event) {
    event.preventDefault();
    $('body').addClass('er-mobile-open');
    // Add class to #er-main to offset margin
    $('#er-main').addClass('er-menu-open');
    // Hide button to replace with close button
    $(this).hide();
    $('#er-menu-opened').show();
    // Slide sidepanel from left
    $("#er-sidepanel").show("slide", {direction: "left"}, er_menu_dur, function() {
      // Reveal top of page button
      $(".er-sidepanel-top").animate({"opacity":1},er_menu_top_dur);
    });
    // Reset menu height
    $('#er-sidepanel-wrapper .er-menu-drilldown').css({"height": $('.er-menu-active > ul').prop('scrollHeight')});
  });
  // Close button
  $("#er-menu-opened").click(function(event) {
    event.preventDefault();
    $('body').removeClass('er-mobile-open');
    // Hide button to replace with menu button
    $(this).hide();
    $('#er-menu-closed').show();
    // Hide to top button and slide sidepanel to left
    $(".er-sidepanel-top").animate({"opacity":0},er_menu_top_dur, function() {
      $("#er-sidepanel").hide("slide", {direction: "left"}, er_menu_dur, function() {
        // Add class to #er-main to reset margin
        $('#er-main').removeClass('er-menu-open').removeAttr('style');
      });
    });
  });




  // Add active class based on page class suffix
  var er_last_page           = $("#er-wrapper-last-page").attr("class");
  var er_last_page_item      = $('.' + $('#er-sidepanel-levels').attr('class').replace(/\s/g, '.') + '[data-page="' + $("#er-wrapper-last-page").attr("class") + '"]');

  if ($("#er-wrapper").hasClass("er-say") == true) {
    $('.er-sidepanel-icon-list .er-say').addClass('er-menu-active');
  }

  er_last_page_item.addClass("er-menu-active");
  er_last_page_item.parents('.er-menu-item').addClass("er-menu-active");

  $(".er-sidepanel-icons ." + er_last_page).addClass("er-menu-active");

  // Forward into the nav if not .er-menu-parent-selected > .er-menu-link (prevents title moving left on click)
  $(".er-menu-drilldown li:not(.er-menu-nochild) > .er-menu-link").click(function(event) {

    // Prevent click
    event.preventDefault();

    // Animate parent column to the left
    $('.er-sidepanel-icons, .er-sidepanel-social').animate({"margin-left":"-"+sidepanel_width},sidepanel_dur);
    $(this).parents().eq(1).find('> li').animate({"margin-left":"-"+sidepanel_width},sidepanel_dur);
    $(this).css('position', 'absolute').animate({"margin-left":"-"+sidepanel_width},sidepanel_dur);

    // Animate child column into view and make visible
    $(this).next('.er-menu-list-child').show().animate({"left":0},sidepanel_dur).addClass("er-menu-list-child-selected");

    // Tabindex change
    $(this).next('.er-menu-list-child').find('> li > a').removeAttr('tabindex');
    $(this).parents().eq(1).find('> li > a').attr('tabindex', '-1');

    // Set height of sidepanel column to reflect absolutely positioned menu items
    $('#er-sidepanel-wrapper .er-menu-drilldown').css({"height": $(this).next().prop('scrollHeight')});

    // Social icons animation
    $(".er-sidepanel-social").css('opacity', 0).animate({"opacity":1},er_menu_top_dur);

    er_sidepanel_bounce()
  });

  // Back button
  $(".er-menu-back-button").click(function(event) {

    // Prevent click
    event.preventDefault();

    // Animate parent column into view
    $(this).parents().eq(5).siblings('.er-sidepanel-icons, .er-sidepanel-social').animate({"margin-left":0},sidepanel_dur);
    $(this).parents().eq(3).find('> li').animate({"margin-left":0},sidepanel_dur);
    $(this).parents().eq(2).find('> .er-menu-link').css('position', 'relative').animate({"margin-left":0},sidepanel_dur);

    // Animate child column to the right and hide
    $(this).parents().eq(1).animate({"left":sidepanel_width},sidepanel_dur).removeClass("er-menu-list-child-selected");

    // Tabindex change
    $(this).parents().eq(1).find('> li > a').attr('tabindex', '-1');
    $(this).parents().eq(3).find('> li > a').removeAttr('tabindex');

    // Set height of sidepanel column to reflect absolutely positioned menu items
    $('#er-sidepanel-wrapper .er-menu-drilldown').css({"height": $(this).parents().eq(3).prop('scrollHeight')});
    // Reset height if top level
    if ($(this).parents().eq(4).hasClass('custom-menu')) {
      $('#er-sidepanel-wrapper .er-menu-drilldown').css({"height":""});
    }

    // Social icons animation
    $(".er-sidepanel-social").css('opacity', 0).animate({"opacity":1},er_menu_top_dur);

    er_sidepanel_bounce()
  });

  $(".er-menu-active").find("> .er-menu-link").click();

});

$(window).resize(function() {
  $(".er-menu-active").find("> .er-menu-link").click();
})

function er_sidepanel_bounce() {
  var window_height       = $(window).height();
  var header_height       = $('#er-header').height();
  var er_totop_height     = $('.er-sidepanel-top').height();
  if ($(window).width() < 960) {
    var er_totop_height     = 0;
  }
  var er_sidepanel_height = $('.er-sidepanel-social').height() + $('.er-sidepanel-icons').height() + $('.er-menu-drilldown').height() + $('.er-sidepanel-social').height() + er_totop_height;

  $('#er-sidepanel-wrapper').css({"min-height": window_height + "px"});

  if ($(window).scrollTop() > header_height) {
  
    if (er_sidepanel_height > window_height) {
  
      if ($(window).scrollTop() >= ((er_sidepanel_height - window_height) + header_height)) {
        $('#er-sidepanel-wrapper').css({"position": "fixed", "padding-bottom": er_totop_height, "top": "auto", "bottom": 0,});
      }
  
      if ($(window).scrollTop() < ((er_sidepanel_height - window_height) + header_height)) {
        $('#er-sidepanel-wrapper').css({"position": "absolute", "bottom": "auto", "top": 0});
      }
    }
  
    else {
      $('#er-sidepanel-wrapper').css({"position": "fixed", "top": 0, "bottom": 0});
    }
  }
  else {
    $('#er-sidepanel-wrapper').css({"position": "absolute", "top" : 0, "bottom": "auto"});
  }
}
$(document).ready(function() {
  er_sidepanel_bounce();
});
$(window).resize(function() {
  er_sidepanel_bounce();
});
$(window).scroll(function(){
  er_sidepanel_bounce();
});

</script>
