        var setMobilePanels = false;
        var loadedFNImages = false;
        var resizeId;
        var lazyloadID;

        jQuery(document).ready(function() {
            var width = jQuery(window).width();
            if (width < 769) {
                panelFunctions('set');
            }
        });

        function fastnewsScrollEvent(){
            if(setMobilePanels && !loadedFNImages){
                var scrollHeight = jQuery(window).scrollTop();
                if(scrollHeight > 2500){
                    loadedFNImages = true;
                    //Load images here
                    mobile_fastnews(true);
                    jQuery( window ).off( ".lazyfastnews");
                    clearTimeout(resizeId);

                }
            }
        }

        function panelFunctions(func){
            jQuery(".mobile-div").each(function(){
                var mobile_id = jQuery(this).attr("id");
                var desktop_id = mobile_id.replace("-mobile", "");
                switch(func){
                    case "set":
                        movePanel(desktop_id);
                        if(desktop_id === "fast-news"){
                            mobile_fastnews(false);
                            if(!loadedFNImages){
                                jQuery( window ).on( "scroll.lazyfastnews",function() {
                                    clearTimeout(lazyloadID);
                                    lazyloadID = setTimeout(fastnewsScrollEvent, 250);
                                });
                            }
                        }
                        showMobilePanel(desktop_id);
                        setMobilePanels = true;
                        break;
                    case 'hide':
                        hideMobilePanel(desktop_id);
                        break;
                    case 'show':
                        showMobilePanel(desktop_id);
                        break;
                    default:
                        hideMobilePanel(desktop_id);
                }
            });
        }

        function mobile_fastnews(loadImages){
            jQuery("#fast-news-mobile > div.vertical-widget-content > div.item").each(function () {
                if(loadImages){
                    //Lazy load image
                    var thumbnail_obj = jQuery(this).find('img');
                    jQuery(thumbnail_obj).css("display","block");
                    jQuery(thumbnail_obj).attr("src",jQuery(thumbnail_obj).data("src"));

                }else{
                    var news_obj = jQuery(this).data('article');
                    var newsCard = build_mobile_fastnews(this,news_obj);
                    if(newsCard !== false){
                        jQuery(this).html(newsCard);
                    }
                }
            });
            setMobilePanels = true;
        }

        function build_mobile_fastnews(element,article_obj){

            if(typeof article_obj === 'undefined'){
                return false;
            }

            var anchorTag = jQuery(element).find('.title').html();
            var anchor_tag_end = anchorTag.indexOf('>');
            var title = jQuery(element).find('.title > a').text();

            var output = '<div><div class="col-lg-2 col-md-2 col-sm-3 col-xs-3 clear-padding">';

            output += anchorTag.substring(0,anchor_tag_end+1);

            if(article_obj.hasThumbnail == true){
                output += '<img src="" data-src="'+article_obj.thumbnailSrc+'" style="display:none;" alt="'+title+'" title="'+title+'">';
            }

            output += '</a></div><div class="col-lg-10 col-md-10 col-sm-9 col-xs-9"><h3 class="title">';
            output += anchorTag;
            output += '</h3><div class="meta m0500"><span class="grey-text small">';
            output +='<span class="inline-block">'+article_obj.date+'</span>';
            output +='<span class="inline-block">&nbsp;/&nbsp;<span class="mny-light-blue">'+article_obj.author+'</span>';
            output +='</span></span></div>';
            output +='</div></div>';

            return output;

        }

        
        function movePanel(id){
            jQuery("#"+id+"-mobile").html(jQuery("#"+id).html());
        }

        function hideMobilePanel(id){
            jQuery("#"+id).css("display","block");
            jQuery("#"+id+"-mobile").css("display","none");
        }

        function showMobilePanel(id){
            jQuery("#"+id).css("display","none");
            jQuery("#"+id+"-mobile").css("display","block");
        }

        window.addEventListener('resize', function() {
            clearTimeout(resizeId);
            resizeId = setTimeout(completedWindowResizing, 500);
        });

        function completedWindowResizing(){
            var width = jQuery(window).width();
            if (width < 769) {
                //Small screen
                if(!setMobilePanels){
                    panelFunctions('set');
                }
                panelFunctions('show');
            }else if(width > 780){
                // Big screen
                panelFunctions('hide');
            }
        }
    </script>
