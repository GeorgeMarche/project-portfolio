<script>
  $(document).ready(function () {   
    // Current date variables
    var currentDate = new Date();
    var currentDay = currentDate.getDay();
    var currentHours = currentDate.getHours();
    var currentMins = currentDate.getMinutes();
    
    // Chat operation date variables
    var dayMin = 1;
    var dayMax = 5;
    
    var hourMin = 8;
    var hourMax = currentDay < 5 ? 16 : 15;  
    
    var minsMin = 29;
    var minsMax = currentDay != 4 ? 45 : 15;
    
    // Check is in operation
    if (currentDay >= dayMin && currentDay <= dayMax &&
        (currentHours > hourMin || (currentHours == hourMin && currentMins > minsMin)) &&
        (currentHours < hourMax || (currentHours == hourMax && currentMins < minsMax))) {
        
        // If it is, show the window
        $(".webchat").show();
      
        $(".webchat .cross").click(function(e) {
          e.preventDefault();
          e.stopPropagation();
          
          $("#c_status_container").animate({
                "height" : "53px",
                "width" : "68px"        
                }, 300);
          $(".webchat .cross").animate({ "width" : 0, "opacity" : 0 }, 500, function() { $(this).hide() });
          $("#c_status_container img").animate({ "padding-left" : 0 }, 500, function() {            
            $("#c_status_container .status-icon").hide();
         });
        });
      
        $(".webchat").click(function(e) {
          e.preventDefault();
          
          var windowSizeArray = [ "width=200,height=200","width=485,height=700,scrollbars=no" ];
          var url = "https://www.tameside.gov.uk/WebChat/tcDefault.aspx";
          var windowName = "popUp";
          var windowSize = windowSizeArray[$(this).attr("rel")];          
          window.open(url, windowName, windowSize);          
        });      
    }
  });
</script>

