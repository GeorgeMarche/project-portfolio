     $('#wat_cover'+img_post_id).hide()
     var buttonExpand = document.createElement("button");       
    buttonExpand.classList.add("clickable");
        
    var textnode = document.createTextNode("Show Full Post");
    buttonExpand.appendChild(textnode);
    buttonExpand.setAttribute("ID","button_colapse"+img_post_id);      
    buttonExpand.addEventListener("click", function(){
       document.getElementById("img_container"+img_post_id).style.height="";
       document.getElementById("button_colapse"+img_post_id).style.display="none";       
       $('#wat_cover'+img_post_id).show()
   });

  

    img.parentNode.parentNode.parentNode.insertBefore(buttonExpand, img.parentNode.parentNode.nextSibling);
  }
  
}



