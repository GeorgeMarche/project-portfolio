<script>
    function socialArticles(request, id, type) {
        let wrap = document.querySelector('#' + id);
        let items = [];
        $.getJSON({
            url: request,
            type: "GET",
            success: function (data) {
                items = data.items;
                console.log(items)
                generateArticles();
            }
        });

        function generateArticles() {
            for (var i = 0; i < items.length; i++) {
                let article = document.createElement('article');

                article.classList.add('social-card');
                article.classList.add('-' + items[i].type);

                if (items[i].type === 'instagram') {
                    article.innerHTML =
                        '<a href="' + items[i].permalink + '" target="_blank" class="social-card__link composite-link">' +
                        '<picture class="social-card__image composite-link__image">' +
                        getImage(items[i].img.url) +
                        '</picture>' +

                        '<div class="social-card__wrapper">' +

                        '<header class="social-card__header">' +
                        getImage(items[i].userpic, 'social-card__header-logo', items[i].nickname) +
                        ' <p class="social-card__header-title">@' + items[i].nickname + '</p>' +
                        ' <p class="social-card__header-subtitle">' + items[i].screenname + '</p>' +
                        '</header>' +

                        getText(items[i].text, items[i].type) +

                        '<p class="social-card__readmore composite-link__title">Afficher la suite</p>' +
                        '</div>' +

                        '<footer class="social-card__footer">' +
                        '<p class="social-card__footer-type">' + items[i].type + '</p>' + (items[i].timestamp ?
                        ' - <p class="social-card__footer-posted-on"><time datetime="' + date.getFullDate(items[i].timestamp) + '"' +
                        'title="' + date.getMonthDate(items[i].timestamp) + '">' + '1' + 'h</time></p>' : '') +
                        '</footer>' +

                        '</a>'
                } else if (items[i].type === 'twitter') {
                    article.innerHTML =
                        '<a href="' + items[i].permalink + '" target="_blank" class="social-card__link composite-link">' +
                        '<div class="social-card__wrapper">' +
                        '<header class="social-card__header">' +

                        getImage(items[i].userpic, 'social-card__header-logo') +

                        ' <p class="social-card__header-title">' + items[i].nickname + '</p>' +
                        ' <p class="social-card__header-subtitle">' + items[i].screenname + '</p>' +
                        '</header>' +

                        getText(items[i].text, items[i].type) +

                        '<p class="social-card__readmore composite-link__title">Afficher la suite</p>' +
                        '</div>' +

                        '<footer class="social-card__footer">' +
                        '<p class="social-card__footer-type">' + items[i].type + '</p>' +  (items[i].timestamp ?
                        ' - <p class="social-card__footer-posted-on"><time datetime="' +  date.getFullDate(items[i].timestamp) + '"' +
                        'title="' + date.getMonthDate(items[i].timestamp) + '">' + '1' + 'h</time></p>' : '') +
                        '</footer>' +

                        '</a>'
                }

                wrap.appendChild(article);
            }
        }

        function getImage(url, classes, alt) {
            let clsList = '';
            let altText = 'image description';

            if (classes !== undefined) {
                clsList = classes;
            }

            if (alt !== undefined) {
                altText = alt;
            }

            if (url.length == 0) {
                return '<img class="'+ clsList +' -broken-img" src="/" alt="" height="0">'
            } else {
                return '<img class="'+ clsList + '" src="' + url + '"alt= "' + altText + '">'
            }
        }

        var date = {
            getFullDate: function (date) {
                const d = new Date(date);
                return d.getFullYear() + '/' + d.getMonth() + '/' + d.getDate();
            },
            getMonthDate: function (date) {
                const d = new Date(date);
                const monthNames = ["January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December"];
                return monthNames[d.getMonth()] + ' ' + d.getDate();
            },
            getHour: function (date) {
                const d = new Date(date);
                return d.getHours();
            }
        }

        function getText(val, type) {
            const regLinks = /(#[^\<]+)/g;
            const regText = /(<([^>]+)>)/ig;
            let length;

            if (type === 'twitter') {
                length = 200;
            } else {
                length = 60;
            }

            function getTags() {
                return val.match(regLinks).slice(0, 5).join(' ');
            }

            function getText(){
                if (type === 'twitter') {
                    return truncate(val.replace(regText , ""));
                } else {
                    return truncate(val.split('<a')[0]);
                }
            }

            function truncate(string){
                if (string.length > length)
                    return string.substring(0,length)+'...';
                else
                    return string;
            };

            if (type === 'instagram') {
                return '<p>'+ getText() + '</p> <p class="hashtag">'+ getTags() +'</p>';
            } else if (
                type === 'facebook' ||
                type === 'youtube' ||
                type === 'twitter') {
                return '<p>'+ getText() + '</p>'
            }
        }
    }
    $( document ).ready(function() {
                    socialArticles('https://www.corbeil-essonnes.fr/wp-admin/admin-ajax.php?action=fetch_posts&stream-id=3', 'stab1');
                            socialArticles('https://www.corbeil-essonnes.fr/wp-admin/admin-ajax.php?action=fetch_posts&stream-id=2', 'stab3');
                            socialArticles('https://www.corbeil-essonnes.fr/wp-admin/admin-ajax.php?action=fetch_posts&stream-id=1', 'stab5');
            });
</script>


