        <script>
        $(document).ready(function() {
            
            // Statistiques live
            $.get("/stats", function(data)
            {
                if(data)
                {
                    $(".statistiques_nombre_total_visites").html(data.nb_total.toString());
                    $(".statistiques_nombre_visites_jour").html(data.nb_jour.toString());
                    $(".statistiques_nombre_visiteurs_live").html(data.nb_connectes.toString());
                    if(data.last_maj != "")
                    {
                        $(".statistiques_date_derniere_maj").html("Mis &agrave; jour le "+data.last_maj);
                        $("#statistiques_date_derniere_maj_li").show();
                    }
                    if(data.date_creation != "")
                    {
                        $(".statistiques_date_creation").html("Cr&eacute;&eacute; le "+data.date_creation.toString());
                        $("#statistiques_date_creation_li").show();
                    }
                }

            }, "json");
            
            
