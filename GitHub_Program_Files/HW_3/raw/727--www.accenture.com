			if(results.left == true){
				console.log("Left")
				$(".slide-right").trigger("click");
			}
		});
		
		var slideTo = 0;
		$("#marqueeCarousel .carousel-inner").onSwipe(function(results){
			if(results.right == true){
				slideTo--;
				if(slideTo<=0){
					slideTo = 0;
				}
				$(".carousel-indicators li[data-slide-to='"+slideTo+"']" ).trigger("click");
			}
			if(results.left == true){
				slideTo++;
				if(slideTo>=3){
					slideTo = 3;
				}
				$(".carousel-indicators li[data-slide-to='"+slideTo+"']" ).trigger("click");
			}
		});
	}
});

