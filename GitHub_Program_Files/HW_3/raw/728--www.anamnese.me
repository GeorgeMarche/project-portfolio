<script>
    var mixin_connection = {
        data: () => ({
            dialog_connection: false,
            valid_connection: false,
            loading_connection: false,
            username: null,
            password: null,
            show_password: false,
            rules_connection: {
                'required': [v => !!v || 'Champs Obligatoire'],
            },
        }),
        methods: {
            validate_connection: function(e){
                if (this.$refs.form_connection.validate()) {
                    this.loading_connection = true;
                    return true
                }
                e.preventDefault();
            },
            reset_dialog_connection: function(){
                this.$refs.form_connection.reset();
                this.dialog_connection = false;
                this.loading_connection = false;
            },
        }
    };
</script>


        




