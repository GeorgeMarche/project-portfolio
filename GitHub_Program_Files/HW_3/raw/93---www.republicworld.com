<script type="text/javascript">
    $(document).ready(function() {
        $(".card").click(function() {
            if (!localStorage.getItem("setPledge")) {
                $.post("https://jarvis.republicworld.com/honda/pledgeCompaign", {
                    setPledge: "true"
                }, function(data, status) {
                    var PledgeStatus = JSON.parse(data)
                    if (PledgeStatus.status) {
                        localStorage.setItem("setPledge", "true");
                    }
                });
            }
        });
        if (localStorage.getItem("setPledge")) {
            $('.card').toggleClass('flipped');
            $('.front').css('display', 'none');
            $('#front-shadow').css('display', 'none');
            getPledgeCount();
        }
    });

    function flip() {
        getPledgeCount();
        if (!localStorage.getItem("setPledge")) {
            $('.card').toggleClass('flipped');
        }
        $('.front').css('display', 'none');
        $('#front-shadow').css('display', 'none');
    }

    function getPledgeCount() {
        $.get("https://jarvis.republicworld.com/honda/pledgecount", function(data, status) {
            parseData = JSON.parse(data);
            var number = parseData.data.totalPledge;
            // alert(number)
            var cast = number.toString(10).split('');
            // alert($('.countMeter').find('span').length)
            if ($('.countMeter').find('span').length < cast.length) {
                for (var i = 0, n = cast.length; i < n; i++) {
                    document.getElementsByTagName('span').innerHTML = cast[i]
                    $(".countMeter").append("<span>" + cast[i] + "</span>");
                }
            }
        });
    }
</script>	</div>
