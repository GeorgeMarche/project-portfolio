    function moveNeedle() {
        rating = 1
        // take the initial deg from the front end by manual after all datapoints where calculated with the help of initial deg
        initial_deg = 248

        // difference between two color legth
        difference_deg = (360-initial_deg)/2

        // to make needle center of the color
        make_needle_center_deg = difference_deg/2

        if (rating == 3) {
