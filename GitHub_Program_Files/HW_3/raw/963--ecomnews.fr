ty();$('.modal_research_count_results_tags').html('Aucun tag');$('.modal_research_count_results_tags_span').html(' ne correspond à votre recherche');$('.modal_research_count_results_articles').html('Aucun article');$('.modal_research_count_results_articles_span').html(' ne correspond à votre recherche');}}$(function(){$('#ipt_modal_research').on('keyup',function(){launchResearch();});$('.modal_research_more_tag').on('click',function(){$('.modal_research_all_tag').slideToggle(300);});if($('#ipt_modal_research').val().length>0){launchResearch();}$(".modal_research").on('shown.bs.modal',function(){$('#ipt_modal_research').focus();});});</script>
