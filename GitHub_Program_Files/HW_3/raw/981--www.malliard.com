<script type="text/javascript" id="sns_global_scripts">var lastData = "";

jQuery(document).ready(function($) {

    $.ajaxSetup({
        cache: false
    });
    setInterval(function() {
        updateMetaData();
    }, 5000);

    setInterval(function() {
        var timeLeft = (parseInt(endTime) - Date.now()) / 1000;
        if (timeLeft < 0) {
            timeLeft = 0;
        }
        $("#timeRemaining").html(String(timeLeft).toHHMMSS());
    }, 1000);

    $("#logo").load(function() {
        setDescHeight();
    });

    $(window).on("orientationchange", function(event) {
        setTimeout(function() {
            setDescHeight();
        }, 1000);
    });


    function setDescHeight() {
        var logoHeight = $("#logo").height();
        var metaDataShowHeight = $("#metaDataShow").height();
        var metaDataTitleHeight = $("#metaDataTitle").height();
        var combinedHeight = metaDataShowHeight + metaDataTitleHeight;
        var finalHeight = logoHeight;
        if (combinedHeight > logoHeight) {
            finalHeight = combinedHeight;
        }

        $("#metaInfoContainer").css("height", finalHeight + 10);
    }

    function updateMetaData() {
        var newData = "";
        var show = "";
        var title = "";
        var desc = "";
        var items = [];
        var upNextInfo = "";
        $.getJSON("https://www.malliard.com/mediainfo.json", function(result) {
            $.each(result, function(i, field) {
                //$("div").append(field + "<br>");
                //newData = newData + field + "<br>";
                items.push(field);
                newData = items[0] + "<br/>" + items[2] + "<br/>" + items[3];
                show = items[3];
                title = items[0]
                desc = items[2];
                endTime = items[6];
                upNextInfo = items[7] + " : " + items[8];
            });

            if (newData != lastData) {
                $("#metaDataShow").empty();
                $("#metaDataShow").append(show);
                $("#metaDataTitle").empty();
                $("#metaDataTitle").append(title);
                $("#metaDataDesc").empty();
                $("#metaDataDesc").append(desc);
                $("#upNextInfo").empty();
                $("#upNextInfo").append(upNextInfo);
                lastData = newData;
                $("#logo").attr("src", "https://www.malliard.com/logos/" + items[4]);
            }
        });
    }

    String.prototype.toHHMMSS = function() {
        var sec_num = parseInt(this, 10); // don't forget the second param
        var hours = Math.floor(sec_num / 3600);
        var minutes = Math.floor((sec_num - (hours * 3600)) / 60);
        var seconds = sec_num - (hours * 3600) - (minutes * 60);

        if (hours < 10) {
            hours = "0" + hours;
        }
        if (minutes < 10) {
            minutes = "0" + minutes;
        }
        if (seconds < 10) {
            seconds = "0" + seconds;
        }
        return hours + ':' + minutes + ':' + seconds;
    }
    updateMetaData();
});</script><link rel='stylesheet' id='mediaelement-css'  href='https://www.malliard.com/wp-includes/js/mediaelement/mediaelementplayer-legacy.min.css?ver=4.2.13-9993131' type='text/css' media='all' />
